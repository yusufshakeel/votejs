{
  "name": "votejs",
  "version": "0.0.1",
  "description": "This is a voting application using NodeJS.",
  "main": "index.js",
  "scripts": {
    "test": "jest -c jest.config.json",
    "no-circular-dependecies": "npx madge -c ./app",
    "generate-docs": "npx jsdoc -c jsdocs.config.json",
    "lint": "npx eslint -c .eslintrc.json ./index.js ./app",
    "precommit": "npm run no-circular-dependecies && npm run lint && npm run test && npm run generate-docs",
    "dbmigrate:create": "npx db-migrate create",
    "db:up": "npx db-migrate up --verbose",
    "db:down": "npx db-migrate down --verbose",
    "db:reset": "npx db-migrate reset --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yusufshakeel/votejs.git"
  },
  "keywords": [
    "vote",
    "votejs",
    "voting",
    "node",
    "nodejs"
  ],
  "author": "Yusuf Shakeel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yusufshakeel/votejs/issues"
  },
  "homepage": "https://github.com/yusufshakeel/votejs#readme",
  "devDependencies": {
    "eslint": "^7.5.0",
    "jest": "^26.1.0",
    "jsdoc": "^3.6.4"
  },
  "dependencies": {
    "config": "^3.3.1",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "knex": "^0.21.2",
    "moment": "^2.27.0",
    "object-mapper": "^6.2.0",
    "pg": "^8.3.0",
    "uuid": "^8.2.0"
  }
}
