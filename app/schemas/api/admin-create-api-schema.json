{
  "$id": "adminCreateApiSchema",
  "type": "object",
  "required": [
    "firstName",
    "emailId",
    "userName",
    "password",
    "passcode",
    "countryCode"
  ],
  "properties": {
    "firstName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "middleName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "lastName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "emailId": {
      "type": "string",
      "minLength": 5,
      "maxLength": 300
    },
    "userName": {
      "type": "string",
      "minLength": 5,
      "maxLength": 128
    },
    "password": {
      "type": "string",
      "minLength": 8
    },
    "passcode": {
      "type": "string",
      "minLength": 6
    },
    "accountStatus": {
      "type": "string",
      "enum": [
        "ACTIVE",
        "INACTIVE",
        "DELETED"
      ],
      "default": "ACTIVE"
    },
    "gender": {
      "type": "string",
      "enum": [
        "MALE",
        "FEMALE",
        "OTHER",
        "UNDISCLOSED"
      ]
    },
    "countryCode": {
      "type": "string",
      "minLength": 3,
      "maxLength": 3
    }
  }
}
